{
  "name": "gestao-eventos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "server": "node server.js",
    "server:kill": "kill -9 $(lsof -ti:3001) 2>/dev/null || echo 'Porta 3001 livre'",
